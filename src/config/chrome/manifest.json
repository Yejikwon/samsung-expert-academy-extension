{
  "name": "samsung-expert-academy-extension",
  "short_name": "SEAe",
  "version": "$VERSION",
  "manifest_version": 2,
  "author": "SsangWoo",
  "description": "chrome extension for Samsung expert academy",
  "homepage_url": "https://github.com/tkddn204/samsung-expert-academy-extension",
  "default_locale": "ko",
  "icons": {
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage", "tabs", "*://*.swexpertacademy.com/main/*"
  ],
  "optional_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": ["*.woff2", "*.png", "*.gif"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  }
}
